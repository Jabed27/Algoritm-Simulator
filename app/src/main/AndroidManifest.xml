<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.jabed.algorithmsimulator">

    <application
        android:allowBackup="true"
        android:icon="@drawable/aslogo"
        android:label="@string/app_name"
        android:roundIcon="@drawable/aslogo"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Activity2" />
        <activity android:name=".BubbleSortActivity" />
        <activity android:name=".InsertionActivity" />
        <activity android:name=".SelectionActivity"></activity>
    </application>

</manifest>